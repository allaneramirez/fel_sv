<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        
        <record id="view_company_form_fel_sv" model="ir.ui.view">
            <field name="name">res.company.form.fel_sv</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <data>
                    <notebook position="inside">
                        <page string="Extra">
                            <group string="FEL">
                                <group>
                                    <field name="certificador_fel_sv"/>
                                    <field name="establecimiento_sv"/>
                                </group>
                                <group string ="Exportacion">
                                     <field name="recinto_fiscal"/>
                                     <field name="regimen"/>
                                     <field name="codigo_incoterm"/>
                                </group>
                            </group>
                        </page>
                    </notebook>
                </data>
            </field>
        </record>
        
    </data>
</odoo>
